#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main()
{
	int a,b,c,g;
    float d,e,f,h;
    scanf("%d%d\n",&a,&b);
    
    scanf("%f%f",&d,&e);
    c=a+b;
    g=a-b;
    f=d+e;
    h=d-e;
    printf("%d %d\n",c,g);
    printf("%.1f %.1f",f,h);

    
    
    return 0;
}
